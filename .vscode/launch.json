{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "create_data",
            "type": "python",
            "request": "launch",
            "program": "tools/create_data.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "kitti",
                "--root-path",
                "../kitti_sample",
                "--out-dir",
                "../kitti_sample",
                "--extra-tag",
                "kitti_sample",
            ],
            "cwd": "${workspaceFolder}/mmdetection3d",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/mmdetection3d:${PYTHONPATH}",
            }
        },
        {
            "name": "Uvicorn Train",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "src.main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "8000",
                "--ws",
                "websockets",
            ],
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
                "LOG_LEVEL": "DEBUG",
                "DEBUG_APP_DIR": "app_data"
            }
        },
        {
            "name": "Uvicorn Serve",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "src.serve.main:model.app",
                "--host",
                "0.0.0.0",
                "--port",
                "8000",
                "--ws",
                "websockets",
            ],
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
                "LOG_LEVEL": "DEBUG",
                "DEBUG_APP_DIR": "app_data",
                "DEBUG_WITH_SLY_NET": "1",
            }
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
                "LOG_LEVEL": "DEBUG",
            }
        },
    ]
}